{
  "manifest_version": 2,

  "name": "Steam API Tests",
  "description": "Tests for the SteamAPI :)",
  "version": "1.0",

  "permissions": [
        "http://*/*",
        "https://*/*"
    ],

  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
